
page{
  background-color: #f5f5f9
}
.page-body{
  width:100%;
}
.filter-wrap {
  padding: 32rpx 0;
}
.InfoItemList {
  clear: both;
  list-style: none;
  padding: 0;
  margin-bottom: 0;
  overflow: auto;
}
/*悬浮按钮*/
.img-plus-style {  
  height: 70rpx;  
  width: 70rpx;  
  position: fixed;  
  bottom: 220rpx;  
  right: 30rpx;  
  opacity: 0.7
}  
.img-plus-style1 {  
  height: 70rpx;  
  width: 70rpx;  
  position: fixed;  
  bottom: 130rpx;  
  right: 30rpx;   
  opacity: 0.7
}  
.zindex100{z-index: 100}
.yc{opacity: 0}




.InfoItemList .list-item {
  position: relative;
  padding: 32rpx;
  font-size: 0;
  border-bottom: 1px solid #E0E0E0;
}

.b-item-img {
  display: inline-block;
  width: 220rpx;
  height: 130rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}

.b-item-img image {
  width: 100%;
  height: 100%;
}
.b-item-right {
  /* display: inline-block; */
  /* width: calc(80% - 70px); */
  vertical-align: middle;
}

.b-item-right .b-item-title {
  font-size: 16px;
  color: #242424;
  text-align: left;
  margin-bottom: 16rpx;
}
.b-item-right .b-item-des {
  display:block;
  width:100%;
  line-height:40rpx;
  font-size:30rpx;
  color:#242424;
  overflow:hidden;
  text-overflow:ellipsis;
  text-align:justify;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;

}
.b-item-tipRight{
  display:block;
  width:100%;
  line-height:40rpx;
  font-size:30rpx;
  color:red;
  overflow:hidden;
  text-overflow:ellipsis;
  text-align:left;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
}
.b-item-tipRightGrey{
  display:block;
  width:100%;
  line-height:40rpx;
  font-size:30rpx;
  color:green;
  overflow:hidden;
  text-overflow:ellipsis;
  text-align:left;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
}

.b-item-right .b-item-info {
  position: relative;
}
.b-item-right .b-item-type {
  /* float: left; */
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  /* color: #8f8f8f; */
  font-size: 30rpx;
  text-align: left;
}
.InfoItemList .b-view-more {
  height: 88rpx;
  line-height: 88rpx;
  font-size: 30rpx;
  color: #666;
  text-align: center;
}
.no-order{
    width: 100%;
    position: absolute;
    bottom: 0;
    top: 88rpx;
    left: 0;
    right: 0;
    text-align: center;
    padding-top: 203rpx;
    background-color: #f5f5f9;
}
.no-order-img{
    width: 81rpx;
    height: 96rpx;
    margin-bottom: 31rpx;
}
.no-order .text{
    font-size:28rpx;
    color:#999999;
    text-align: center
}
.order-list{
    width: 100%;
}

.Grid {
  display: flex;
}
.Grid-cell, .Grid-cell-sub {
  flex: 1;
  text-align: left;
  color:#ffffff;
  border-radius: 4px;
  height: 41px;
  line-height: 45px;
}
.redText{
  color: red;
}
.dxImage{
  width:91px;
  float:right;
  height:80px;
  margin-top:-27px;
}
.tel{
   text-decoration: underline;
}
.note{
  text-align: left;
  background: #fff2df;
  color:#5F5F5F;
  border-radius: 4px;
  margin: 10px;
  height: 41px;
  line-height: 45px;
}
.journey_title{
  text-align: center;
  margin-left:10px;
  font-size:32rpx;
  margin-right:10px;
}


